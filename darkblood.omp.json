{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#CB4B16",
          "style": "plain",
          "template": "\u250f",
          "type": "text"
        },
        {
          "foreground": "#ff6600",
          "style": "diamond",
          "template": "<#CB4B16>[</>\ufa1e{{ .FormattedMs }}<#CB4B16>]</>",
          "properties": {
            "threshold": 1500,
            "style": "round"
          },
          "type": "executiontime"
        },
        {
          "foreground": "#ffffff",
          "properties": {
            // "home_icon": "\uf7db",
            "style": "full"
          },
          "style": "plain",
          "template": "<#CB4B16><b>[</b></>{{ .Path }}<#CB4B16><b>]</b></>",
          "type": "path"
        },
        {
          "foreground": "#ff6600",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFC600{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "properties": {
            "branch_icon": "\ue725 ",
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#CB4B16>[</>{{ .UpstreamIcon }}{{ .HEAD }}{{ if .Staging.Changed }}<#FF6F00> \uf046 {{ .Staging.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}<#CB4B16>]</>",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#CB4B16",
          "style": "plain",
          "template": "\u2516",
          "type": "text"
        },
        {
          "foreground": "#ff4d00",
          "style": "plain",
          "template": "<b>[</b>{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<b>{{ .UserName }}@{{ .HostName }}</b><#CB4B16><b>]</b></>",
          "type": "session"
        },
        {
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": true
          },
          "style": "diamond",
          "template": "<#CB4B16>[</><#ff8800>\ue235 </>{{ if .Venv }}{{ .Venv }}{{ end }}<#CB4B16>]</>",
          "type": "python"
        },
        {
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": true
          },
          "style": "diamond",
          "template": "<#CB4B16>[</><#ff8800>\ue718 \ue71e </>{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}<#CB4B16>]</>",

          "type": "node"
        },
        {
          "foreground": "#ff4d00",
          "style": "plain",
          "template": " \u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
